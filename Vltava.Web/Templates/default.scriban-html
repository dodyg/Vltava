<html>
<head>
    <link rel=""stylesheet"" type=""text/css"" href=""http://fonts.googleapis.com/css?family=Germania+One"">
    <style>
    body {
        font-family: 'Germania One', serif;
        font-size: 24px;
    }
    </style>
</head>
<body>
    <ul>
    {{ for x in syndications }}
        <li>
            {{ if x.item.title != null }}
                <strong>{{x.item.title}}</strong>
                <a href="{{x.item.links[0].uri}}">#</a>
                <br/>
            {{ end }}

            {{x.item.description}}
            {{ if x.item.title == null }}<a href="{{x.item.links[0].uri}}">#</a>{{ end }}    
            <br/>
            {{x.item.published}}
        </li>
    {{ end }}
    </ul>
</body>
</html>